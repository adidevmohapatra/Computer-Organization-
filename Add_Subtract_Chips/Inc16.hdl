/**
 * 16-bit incrementer:
 * out = in + 1 (arithmetic addition)
 * in is UNSIGNED
 * carry output is intentionally ignored
 */

CHIP Inc16 {
    IN in[16];
    OUT out[16];

    PARTS:
   // Put you code here:
   //Takes the add16 chip and increments it by making a in the input and b=0
    Add16(a=in,b[0]=true,out=out);

}